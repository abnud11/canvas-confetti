{
  "name": "@abnud11/canvas-confetti",
  "version": "2.0.0",
  "main": "dist/canvas-confetti.js",
  "module": "dist/canvas-confetti.mjs",
  "types": "dist/main.d.ts",
  "engines": {
    "node": ">=22.0.0 < 23.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/canvas-confetti.js",
      "require": "./dist/canvas-confetti.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vite build && esbuild --minify dist/canvas-confetti.mjs --outfile=dist/canvas-confetti.mjs --allow-overwrite --format=esm --target=esnext",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.js",
    "type:check": "tsc"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/node": "22",
    "browserslist-config-baseline": "^0.5.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^61.0.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "browserslist": [
    "extends browserslist-config-baseline"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.15.1"
}
